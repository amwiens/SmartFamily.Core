<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0</TargetFrameworks>
    <RootNamespace>SmartFamily.Core</RootNamespace>
    <AssemblyName>SmartFamily.Core</AssemblyName>
    <PackageId>SmartFamily.Core</PackageId>
    <Description>SmartFamily.Core is a library that contains many helpers for dealing with family tree information.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MinVer" Version="2.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
